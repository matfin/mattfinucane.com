@font-face {
	font-family: 'mattfinucane';
	src:url('/fonts/mattfinucane.eot?cmup1o');
	src:url('/fonts/mattfinucane.eot?#iefixcmup1o') format('embedded-opentype'),
		url('/fonts/mattfinucane.woff?cmup1o') format('woff'),
		url('/fonts/mattfinucane.ttf?cmup1o') format('truetype'),
		url('/fonts/mattfinucane.svg?cmup1o#mattfinucane') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'mattfinucane';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-github:before {
	content: "\eab4";
}

.icon-arrow-left:before {
	content: "\e60d";
}

.icon-arrow-right:before {
	content: "\e614";
}
